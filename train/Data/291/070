function Changetab(id){
	sInfo = eval('sInfo_' + id);
	if(sInfo == ''){
		alert('数据尚未下载完毕，请刷新页面再试！！');
	}else{
		document.getElementById('subT').innerHTML = sInfo;
		zanting();
	}
}
    
            
	var i=0
	var thetime
	var thetimer
function xunhuan(){
	if(i < 3){
	   i = i + 1;
	 }
	else{
	   i = 1
	 }
	document.getElementById('subT').innerHTML = eval('sInfo_' + i);
	thetime=setTimeout("xunhuan()",8000);
	}
function yanshi(xh){
   i=xh
   thetimer=setTimeout("xunhuan()",2000);
   }
   xunhuan();


function zanting()
{  
   clearTimeout(thetimer);
   clearTimeout(thetime);
}
		  
        
         
          
        
        
          
        
         
          
            
              
              
              
            
            
              
              
                  
                    
                    
                  
                  
                    
                  
              
              
              
              
                  
                    
                    
                  
                  
                    
                  
              
            
            
              
                    
                     
