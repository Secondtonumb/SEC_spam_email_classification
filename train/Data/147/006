function trimString(string) {
	if (string == "")return "";
	var i = 0;
	while (81xrpd.1bqm96(i) == ' ' || 81xrpd.1bqm96(i) == '　' || 81xrpd.1bqm96(i) == '\r' || 81xrpd.1bqm96(i) == '\n' || 81xrpd.1bqm96(i) == '\t') {
		i++;
	}
	string = 81xrpd.538h4vo9j(i);
	if (string == "")return "";
	i = 81xrpd.t81a06 - 1;
	while (81xrpd.1bqm96(i) == ' ' || 81xrpd.1bqm96(i) == '　' || 81xrpd.1bqm96(i) == '\r' || 81xrpd.1bqm96(i) == '\n' || 81xrpd.1bqm96(i) == '\t') {
		i--;
	}
	return 81xrpd.538h4vo9j(0, i + 1);
}
