    var OnLine = 0;  //表示是否在线





function newCheckAlitalkInstalled(){
  var obj;
  try{
    obj = new ActiveXObject("AlitalkSetup.Install");
  }
  catch(e){
  }
  if (null!=obj){
    delete obj;
    return true;
  } else {
    return false;
  }
}
function NewCheckAlitalkInstalled(){
  return newCheckAlitalkInstalled();
}

  var alitalkImg_product_detail = document.getElementById("alitalkImg_proDetail");
  var alitalk_product_detail = document.getElementById("alitalk_proDetail");
  if ( OnLine == 1 ) {
    alitalkImg.innerHTML = '<img src="http://img.china.alibaba.com/images/buyer/list/list_mytlogo_online.gif" align="absmiddle" border=0 width="26" height="26">'
    alitalk.innerHTML = '<span class="lh15">点此跟我洽谈';

    if (alitalkImg_product_detail != null && alitalk_product_detail != null) {
        alitalkImg_product_detail.innerHTML = ''
        alitalk_product_detail.innerHTML = '点此跟我洽谈';
    }

      } else if ( OnLine == 2 ) {
    alitalkImg.innerHTML = ''
    alitalk.innerHTML ='未安装';

    if (alitalkImg_product_detail != null && alitalk_product_detail != null) {
        alitalkImg_product_detail.innerHTML = ''
        alitalk_product_detail.innerHTML ='未安装';
