    function inputPageNum () {
	    var page = document.getElementById("pageNum").value;
	    if(event.keyCode == 13 && page != null && page != "") {
		    gotoPage();
		}
	}
	function gotoPage(){
			    	    var page = document.getElementById("pageNum").value;
		if (page == null || page == "") {
		    page = 1;
		}
		if (page > 3) {
		    page = 3;
		}
		if (!(page >= 1)) {
		    page = 1;
		}
		var url = "http://xzpack.cn.alibaba.com/athena/offerlist/xzpack-sale-" + page + "-false.html";
		window.location.href = url;
	}


      
        
        
          
