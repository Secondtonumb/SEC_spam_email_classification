  function check1(infoType) {
    //判断浏览者是否安装了贸易通
    if(clientCaps.isComponentInstalled("{8601658B-9360-00C1-AE00-00C03EA25D91}", "componentID") || NewCheckAlitalkInstalled()) {
        var d = new Date();
        if (document.images) {
          (new Image()).src = "http://china.alibaba.com/others/offeralitalkclick.htm?online="+OnLine+"
    	}

        if(infoType=="product"||infoType=="company"){
            window.location = "Alitalk:SendIM?xzpack
        }else{
            window.location = "Alitalk:SendIM?xzpack
        }
    } else {
        //提示安装贸易通
        window.target = "_blank";
      	window.open("http://china.alibaba.com/community/turbine/template/community%2CATCPromotionDown");
    }
  }

  function checkAdd(){
    //判断浏览者是否安装了贸易通
    if(clientCaps.isComponentInstalled("{8601658B-9360-00C1-AE00-00C03EA25D91}", "componentID") || NewCheckAlitalkInstalled()) {
      window.location="Alitalk:AddContact?uid=xzpack";
    }else{
      var obj;
      try{
          obj = new ActiveXObject("AlitalkSetup.Install");
      }catch(e){
      }
      if (null!=obj){
        window.location="Alitalk:AddContact?uid=xzpack";
      } else {
        // 提示安装贸易通
        window.target = "_blank";
        window.open("http://china.alibaba.com/community/turbine/template/community,ATCPromotionDown");
      }
    }
  }



