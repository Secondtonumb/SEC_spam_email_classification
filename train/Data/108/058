  function changeImg(obj,width,height) {
    if ( obj.width > width || obj.height > height ) {
      var scale;
      var scale1 = obj.width / width;
      var scale2 = obj.height / height;

      if(scale1 > scale2){
        scale = scale1;
      }else{
        scale = scale2;
      }

      obj.width = obj.width / scale;
    }
  }



  
  
    
      
        
        
          
