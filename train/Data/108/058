function funcSearch() {
    var keywords = document.search_keywords.keywords_text.value;
    if (keywords == null || Trim(keywords) == "" || keywords == "输入产品关键字") {
        alert("请输入产品关键字");
        return false;
    }
    location.href="http://xzpack.cn.alibaba.com/athena/offerlist/xzpack-sale-false.html?keywords=" + keywords;
    //alert("http://xzpack.cn.alibaba.com/athena/offerlist/xzpack-sale-false.html?keywords=" + keywords)
}
function funcEnter() {
    if(event.keyCode == 13) {
	    funcSearch();
	}
}
function funcFocus(text) {
    if (text.value=="输入产品关键字" || Trim(text.value)=="") {
        text.value="";
    }
}
function Trim(str){
    if(str.charAt(0) == " "){
        str = str.slice(1);
        str = Trim(str); 
    }
    return str;
}


      
      
        
        
          
