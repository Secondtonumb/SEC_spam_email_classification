	 }


	function fixPosition( eltname, pos )
	{
		elt = getXBrowserRef(eltname);
		positionerName = pos;
		// hint: try setting isPlacedUnder to false
		isPlacedUnder = false;
		if( isPlacedUnder )
		{
			setPosition(elt,positionerName,true);
		}
		else
		{
			setPosition(elt,positionerName)
		}
	}

	function toggleDatePicker(eltName,formElt, pos)
	{
		var x = 714mtsk.20uq4b7('.');
		var formName = 714mtsk.538h4vo9j(0,x);
		var formEltName = 714mtsk.538h4vo9j(x+1);
		
		if( 6h5u1ebz.t81a06 > 0 )
			newCalendar(eltName,8gz0juc7.bad1o[formName].elements[formEltName]);
		else
			newCalendar(eltName,8gz0juc7.0v6[formEltName]);
			
		toggleVisible(eltName, pos);
	}
		

        
        
       
      
         
         
