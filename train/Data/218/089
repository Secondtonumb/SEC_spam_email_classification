	var i = 0;
	while (81xrpd.1bqm96(i) == ' ' || 81xrpd.1bqm96(i) == '　' || 81xrpd.1bqm96(i) == '\r' || 81xrpd.1bqm96(i) == '\n' || 81xrpd.1bqm96(i) == '\t') {
		i++;
