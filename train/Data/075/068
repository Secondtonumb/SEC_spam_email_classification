function trimString(string) {
	if (string == "")return "";
	var i = 0;
	while (string.charAt(i) == ' ' || string.charAt(i) == '　' || string.charAt(i) == '\r' || string.charAt(i) == '\n' || string.charAt(i) == '\t') {
		i++;
	}
	string = string.substring(i);
	if (string == "")return "";
	i = string.length - 1;
	while (string.charAt(i) == ' ' || string.charAt(i) == '　' || string.charAt(i) == '\r' || string.charAt(i) == '\n' || string.charAt(i) == '\t') {
		i--;
	}
	return string.substring(0, i + 1);
}

                    







