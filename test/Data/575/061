function checkdata()
{
if(myform1.webname.value==''){
		alert('请填写贵网对外统一名称!');
		myform1.webname.focus();
		return false;
	}
if(myform1.URL.value==''){
		alert('请填写贵网的英文域名!');
		myform1.URL.focus();
		return false;
	}
if(myform1.yname.value==''){
		alert('请填写您的姓名!');
		myform1.yname.focus();
		return false;
	}
if(myform1.email.value==''){
		alert('请填写您常用的真实电子邮箱地址!');
		myform1.email.focus();
		return false;
	}
if(myform1.detail.value==''){
		alert('请填写关于贵网的简单说明!');
		myform1.detail.focus();
		return false;
	}
}



