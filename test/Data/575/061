function checkspace(checkstr) {
  var str = '';
  for(i = 0; i < checkstr.length; i++) {
    str = str + ' ';
  }
  return (str == checkstr);
}

function checkreceiveaddr()
{
   if(checkspace(document.receiveaddr.recepit.value)) {
	document.receiveaddr.recepit.focus();
