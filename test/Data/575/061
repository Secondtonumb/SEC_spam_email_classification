   function check2()
{
   if(checkspace(document.shop2.password1.value)) {
	document.shop2.password1.focus();
    alert("请输入新密码！");
	return false;
  }
  if(checkspace(document.shop2.password2.value)) {
	document.shop2.password2.focus();
    alert("请输入确认密码！");
	return false;
  }
  if(document.shop2.password1.value != document.shop2.password2.value) {
	document.shop2.password1.focus();
	document.shop2.password1.value = '';
	document.shop2.password2.value = '';
    alert("两次输入的密码不同，请重新输入！");
	return false;
  }
}

