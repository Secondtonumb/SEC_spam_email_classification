       function zydn_net_menuover(td_name,td_bg1,td_co1){
           if (td_bg1!==""){ td_name.background=td_bg1;}
           td_name.bgColor=td_co1;
       } 

       function zydn_net_mouseout(td_name,td_bg){
           if ( td_bg!==""){
                td_name.background=td_bg ;}
           else{
                     td_name.background="";
           }
           td_name.bgColor="";
      } 



	


	
		
			

  
		
