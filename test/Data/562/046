function Oncheck()
{
     var inputs = document.getElementById("ctl00_MainContentPlaceHolder_GridView1").getElementsByTagName("INPUT");
     var chk = document.getElementById("ctl00_MainContentPlaceHolder_Chk");
     
     if(chk.checked == true)
     {
        if((inputs != null)&&(inputs !=undefined))
        {
            for(var i = 0; i < inputs.length; i++)
            {
                if(inputs[i].type == "checkbox")
                {
                    inputs[i].checked = true;
                }    
            }
        }
     }
     else
     {
        if((inputs != null)&&(inputs !=undefined))
        {
            for(var i = 0; i < inputs.length; i++)
            {
                if(inputs[i].type == "checkbox")
                {
                    inputs[i].checked = false;
                }    
            }
        }
     }
}

function ClearAll()
{
   var inputs = document.getElementById("ctl00_MainContentPlaceHolder_GridView1").getElementsByTagName("INPUT");
     var chk = document.getElementById("ctl00_MainContentPlaceHolder_Chk");
    
     chk.checked = false;
    for(var i = 0; i < inputs.length; i++)
    {
        if(inputs[i].type == "checkbox")
        {
            inputs[i].checked = false;
        }    
    }
}

    

    
