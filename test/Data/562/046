document.getElementById("ctl00_MainContentPlaceHolder_CheckAuth1_panelAuth").style.display = "none";
var eventSrc = null;
var bAuthed = new Boolean(true);
function GetOffset(element)
{
	var offSet = new Object();
	offSet.X = element.offsetLeft;
	offSet.Y = element.offsetTop;
	
	if(element.offsetParent == document.body)
		return offSet;
	else
	{
		var offsetParent = {X:0, Y:0};
		if(null==element.offsetParent)
		    offsetParent = GetOffset(element.parentElement);
		else
		    offsetParent = GetOffset(element.offsetParent)
		offSet.X += offsetParent.X;
		offSet.Y += offsetParent.Y;
		return offSet;
	}
}

function CheckLoginStatus(e)
{
	if(true == bAuthed)
		return true;

	var panelAuth=document.getElementById("ctl00_MainContentPlaceHolder_CheckAuth1_panelAuth");
	var elementUserName=document.getElementById("ClientUserName");

    if(e)
        eventSrc = e.target;
    else
	    eventSrc = window.event.srcElement;
	var s = eventSrc;
	//s.style.position="absolute";
	var offsetS = GetOffset(s);
	
	var authSize = {Width:0, Height:0};
	authSize.Width = 266;
	authSize.Height = 196;
	//Get event source element center location
	var srcCenter = {X:0, Y:0};
	srcCenter.X = offsetS.X + s.offsetWidth/2;
	srcCenter.Y = offsetS.Y + s.offsetHeight/2;
	//Get auth pantel left/top location
	var authLocal = {X:0, Y:0};
	authLocal.X = srcCenter.X - authSize.Width/2;
	authLocal.Y = srcCenter.Y - authSize.Height/2;
	
	var start = {X:0, Y:0};
    start.X = (document.documentElement 
    start.Y = (document.documentElement 
    var offset = {W:0, H:0};
    offset.W = (document.documentElement 
    offset.H = (document.documentElement 
    //If out of current scroll view left or top, move in
    var fixAuth = {X:0, Y:0};
    fixAuth.Left = Math.max(start.X, authLocal.X);
    fixAuth.Top = Math.max(start.Y, authLocal.Y);

	//if out of current scroll view right or buttom
	if(fixAuth.Left + authSize.Width > start.X + offset.W && offset.W > authSize.Width)
		panelAuth.style.left = start.X + offset.W - authSize.Width;
	else
		panelAuth.style.left = fixAuth.Left;
	if(fixAuth.Top + authSize.Height > start.Y + offset.H && offset.H > authSize.Height)
		panelAuth.style.top = start.Y + offset.H - authSize.Height;
	else
		panelAuth.style.top = fixAuth.Top;
	
	panelAuth.style.display="";
	elementUserName.focus();
	return false;
}	

function OnCheckAuthLogin()
{
	var username=document.getElementById("ClientUserName");
	var password=document.getElementById("ClientPassWord");
	if((username.value!="")&&(password.value!=""))
	{
		if(true == document.getElementById("RememberMe").checked)
			Sys.Services.AuthenticationService.login(username.value, password.value, true, OnChecAuthComplete); 
		else
			Sys.Services.AuthenticationService.login(username.value, password.value, false, OnChecAuthComplete); 
    }
    else
    {
	    var temp3=document.getElementById("checkAuthPromptInfo");
	    temp3.innerHTML="è¾??ä¸????;
	    temp3.style.color="red";
    }
    return false;
}

function OnChecAuthComplete(result)
{
	if(result==true)
	{
		bAuthed = true;
		if(null != eventSrc)
		{
			eventSrc.focus();
			eventSrc.click();
			OnCheckAuthClose();
			//Check if the anonymous login view is displayed in page, refresh for switch it to logined view.
			if(null != document.getElementById("ctl00_LoginView1_AnonymousDisplay_Login1") && true ==true)
			{
				window.setTimeout("window.location.reload()", 2000);
			}
		}
		else
			location.reload();
	}
	else
	{
	    var temp3=document.getElementById("checkAuthPromptInfo");
	    temp3.innerHTML="?¨æ???????è¯?;
	    temp3.style.color="red";
	}
}

function OnCheckAuthClose()
{
	eventSrc = null;
	document.getElementById('ctl00_MainContentPlaceHolder_CheckAuth1_panelAuth').style.display="none";
}

var strClientIDs = "ctl00_MainContentPlaceHolder_BtComment;ctl00_MainContentPlaceHolder_ImageButton1;ctl00_MainContentPlaceHolder_BtCollect;ctl00_MainContentPlaceHolder_BtModify;";
var fnOldClickHandler = new Array();
var fnNewClickHandler = new Array();
if(strClientIDs.length > 0)
{
    var clientIDs = strClientIDs.split(";");
    for(var i=0; i<clientIDs.length; i++)
    {
        var obj = document.getElementById(clientIDs[i])
        if(null != obj)
        {
            fnOldClickHandler[i] = obj.onclick;
            if(null != fnOldClickHandler[i])
            {
                //This is a dynamic function, can't get the local variable i
                fnNewClickHandler[i] = function() 
                    {
                        if(false == CheckLoginStatus()) 
                            return false;
                        var n = 0;
                        for(n=0; n<fnNewClickHandler.length; n++)
                        {
                            //Can't get local i value in new function, need to find the orginal index first.
                            var fnObj = fnNewClickHandler[n];
                            if(fnObj === this.onclick)
                                break;
                        }
                        if(null != fnOldClickHandler[n])
                            return fnOldClickHandler[n](); 
                        else
                            return true;
                    }
                obj.onclick = fnNewClickHandler[i];
            }
            else
                obj.onclick = function() {return CheckLoginStatus();}
        }
    }
}




            

