 function closeWin()
 {
    var popupMask = document.getElementById("popupMask");
	popupMask.style.display = "none";
    
    var p1 = document.getElementById("popupContainer");
    p1.style.display = "none";
   
	window.onresize=closeWin; 
    window.onscroll = closeWin;
 } 
/*
* å¾??å½??è§??ä¸?????é«?º¦???åº?
*/
function getViewportHeight() {
	if (window.innerHeight!=window.undefined) return window.innerHeight;
	if (document.compatMode=='CSS1Compat') return document.documentElement.clientHeight;
	if (document.body) return document.body.clientHeight; 
	return window.undefined; 
}
function getViewportWidth() {
	if (window.innerWidth!=window.undefined) return window.innerWidth; 
	if (document.compatMode=='CSS1Compat') return document.documentElement.clientWidth; 
	if (document.body) return document.body.clientWidth; 
	return window.undefined; 
}
/*
*  ä¸ºæ?ä»¶å????ç§»é?äº?»¶
*/

function addEvent(obj, evType, fn){
 if (obj.addEventListener){
    obj.addEventListener(evType, fn, true);
    return true;
 } else if (obj.attachEvent){
    var r = obj.attachEvent("on"+evType, fn);
    return r;
 } else {
    return false;
 }
}
function removeEvent(obj, evType, fn, useCapture){
  if (obj.removeEventListener){
    obj.removeEventListener(evType, fn, useCapture);
    return true;
  } else if (obj.detachEvent){
    var r = obj.detachEvent("on"+evType, fn);
    return r;
  } else {
    alert("Handler could not be removed");
  }
}



function centerPopWin(width, height) {
		if (width == null || isNaN(width)) {
			width = popupContainer.offsetWidth;
		}
		if (height == null) {
			height = popupContainer.offsetHeight;
		}
		
		var p = document.getElementById("ctl00_MainContentPlaceHolder_addRef_P3");
        
        if(p.style.display = "none")
        {
            p.style.display = ""; 
        }
        
		var fullHeight = getViewportHeight();
		var fullWidth = getViewportWidth();
		
		var theBody = document.documentElement;
		
		var scTop = parseInt(theBody.scrollTop,10);
		var scLeft = parseInt(theBody.scrollLeft,10);
		
		var popupMask = document.getElementById("popupMask");
		
		popupMask.style.display = "";
		popupMask.style.height = fullHeight + "px";
		popupMask.style.width = fullWidth + "px";
		popupMask.style.top = scTop + "px";
		popupMask.style.left = scLeft + "px";
		
		var p1 = document.getElementById("popupContainer");
        p1.style.display = "";
		
		var titleBarHeight = parseInt(document.getElementById("popupTitleBar").offsetHeight, 10);
		
		popupContainer.style.top = (scTop + ((fullHeight - (height+titleBarHeight)) / 2)) + "px";
		popupContainer.style.left =  (scLeft + ((fullWidth - width) / 2)) + "px";
		
		window.onresize = centerPopWin;
		window.onscroll = centerPopWin;
}
//addEvent(window, "resize", closeWin);
 
function enableControl()
{
	var f = document.getElementById("ctl00_MainContentPlaceHolder_addRef_FileUpload1");
    var r1 = document.getElementById("ctl00_MainContentPlaceHolder_addRef_raUpFile");
    var r = document.getElementById("ctl00_MainContentPlaceHolder_addRef_raAddUrl");
    var u = document.getElementById("ctl00_MainContentPlaceHolder_addRef_TxtUrl");
    
    if(r1.checked == true)
    {
        f.disabled = false;
        u.disabled = true;
        r.checked = false;
    }
}

function enControl()
{
    var f = document.getElementById("ctl00_MainContentPlaceHolder_addRef_FileUpload1");
    var r1 = document.getElementById("ctl00_MainContentPlaceHolder_addRef_raUpFile");
    var r = document.getElementById("ctl00_MainContentPlaceHolder_addRef_raAddUrl");
    var u = document.getElementById("ctl00_MainContentPlaceHolder_addRef_TxtUrl");
    
    if(r.checked == true)
    {
        f.value = "";
        f.disabled = true;
        u.disabled = false;
        r1.checked = false;
    }
}
 


	



    
        
            
            
